{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint", "prettier", "eslint-plugin-tsdoc"
    ],
    "extends": ["airbnb-base",
        "airbnb-typescript/base",
        "prettier"],
    "rules": {
        "prettier/prettier": ["warn"],
        "no-console": 0
    }
}
